{
  "name": "telegraf-ex",
  "version": "0.1.0",
  "description": "Extensions to telegraf framework",
  "main": "index.js",
  "types": "index.d.ts",
  "scripts": {
    "patch": "tsc && npm version patch",
    "major": "tsc && npm version major",
    "minor": "tsc && npm version minor",
    "build": "tsc"
  },
  "keywords": [
    "telegram",
    "telegraf",
    "dialogs",
    "commands",
    "polls"
  ],
  "author": "Damir Yusipov",
  "license": "MIT",
  "dependencies": {
    "telegraf": "^3.38.0",
    "mongoose": "^5.10.13"
  },
  "devDependencies": {
    "@types/node": "^14.14.7",
    "@types/mongoose": "^5.7.37",
    "typescript": "^3.9.7",
    "nodemon": "^1.19.4",
    "ts-node": "^8.10.2",
    "tslint": "^5.20.1"
  },
  "jest": {
    "transform": {
      ".(ts|tsx)": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ]
  }
}
